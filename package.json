{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "devstart": "babel-node .src/bin/www",
    "start": "npm run build && node ./build/bin/www",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src -s",
    "clean": "rm -rf ./build && mkdir ./build"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  }
}
